{
  "name": "@mf-app/remote",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:types": "tsc",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "serve": "vite build --mode=local-prod && vite preview --host --mode=local-prod",
    "postinstall": "npm install --no-save @mf-app/store@file:../mf-app-store/dist/types",
    "twlit": "twlit --input ./src/tailwind.css --output ./src/twlit.js",
    "tw:update": "tailwindcss -i ./src/index.css -o ./src/tailwind.css"
  },
  "dependencies": {
    "@felte/solid": "^1.2.13",
    "@felte/validator-zod": "^1.0.17",
    "@lit/react": "^1.0.5",
    "lit": "^3.1.3",
    "solid-js": "^1.8.17",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@originjs/vite-plugin-federation": "^1.3.5",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "twlit": "^0.0.1-alpha.37",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-solid": "^2.10.2",
    "vite-tsconfig-paths": "^4.3.2",
    "zustand": "^4.5.2"
  }
}
